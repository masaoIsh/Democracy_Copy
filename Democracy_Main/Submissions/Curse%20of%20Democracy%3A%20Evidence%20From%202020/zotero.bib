@article{burkhart,
	title = {Comparative democracy: {The} economic development thesis},
	volume = {88},
	url = {https://EconPapers.repec.org/RePEc:cup:apsrev:v:88:y:1994:i:04:p:903-910_09},
	abstract = {In comparative politics, an established findingâ€”that economic development fosters democratic performanceâ€”has recently come under challenge. We counter this challenge with a dynamic pooled time series analysis of a major, but neglected data set from 131 nations. The final generalized least squares-autoregressive moving averages estimates (N = 2,096) appear robust and indicate strong economic development effects, dependent in part on the nation's position in the world system. For the first time, rather hard evidence is offered on the causal relationship between economics and democracy. According to Granger tests, economic development â€œcausesâ€ democracy, but democracy does not â€œcauseâ€ economic development. Overall, the various tests would seem to advance sharply the modeling of democratic performance.},
